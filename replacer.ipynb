   "source": [
    "FromTo = tuple[str, str]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "1109e07b-67f9-4c2e-b94c-091e319000e1",
   "metadata": {},
   "outputs": [],
   "source": [
    "def zip_replace(text: str, changes: Iterable[FromTo]) -> str:\n",
    "    for from_, to in changes:\n",
    "        text = text.replace(from_, to)\n",
    "    return text"
   ]
